<%@ page language="java" contentType="text/html; charset=UTF-8"	pageEncoding="UTF-8"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><%@ taglib uri="http://www.springframework.org/security/tags" prefix="security"%><%@ taglib uri="http://java.sun.com/jsp/jstl/functions" prefix="fn"%><!DOCTYPE html><html><head><title>TestCenter New Company</title><link rel="stylesheet" href="css/style.css" type="text/css"	media="screen" /></head><body>	<div id="container">		<div id="header">			<a href="index.html"><img src="images/title.png"></a>		</div>		<a href="index.html">Back To Testcenter</a>		<div id="content">			<c:if test="${fn:length(displaymessage) > 0}">				<font color="red">${displaymessage}</font>				<hr>			</c:if>					<fieldset>			<legend>${company.companyName}</legend>				<table width="600" border="0">								<tr>				<td>										<table width="100%">						<tr>									<td>								<table  width="100%" cellpadding="3" cellspacing="3">								<tr>																		<th style="color: white;background-color: #6199DF;border: 1px solid #4D90FE;">										Resource 									</th>									<th style="color: white;background-color: #6199DF;border: 1px solid #4D90FE;">										Total									</th>									<th style="color: white;background-color: #6199DF;border: 1px solid #4D90FE;">										Unit Price									</th>									<th style="color: white;background-color: #6199DF;border: 1px solid #4D90FE;">										Total Price									</th>								</tr>								<tr  >									<td>										${company.projectsDisplayName} 									</td>									<td align="right">										<c:set var="companyProjects" value="0" scope="page" />																				<c:set var="companyProjects" value="${companyProjects + fn:length(company.projects)}" scope="page"/>												${companyProjects}									</td>									<td align="right">										<c:set var="projectPrice" value="5.12 " scope="page" />												&#8364;${projectPrice} / Month<!-- &#8364; 0.12/hour -->									</td>									<td align="right">										<c:set var="totalProject" value="${companyProjects * projectPrice}" scope="page" />												&#8364;${totalProject}									</td>								</tr>																	<c:set var="cyclecount" value="0" scope="page" />																		<c:forEach var="project" items="${company.projects}" varStatus="projectIndex" >											<c:set var="cyclecount" value="${cyclecount + fn:length(project.cycles)}" scope="page" />																			</c:forEach>																<tr style="background-color:#edf7f9" >									<td>										${company.cyclesDisplayName} 									</td>									<td align="right">																					${cyclecount}									</td>									<td align="right">										<c:set var="cyclePrice" value="1.49 " scope="page" />												&#8364;${cyclePrice} / Month<!-- &#8364; 0.12/hour -->									</td>									<td align="right">																				<c:set var="totalCycle" value="${cyclecount * cyclePrice}" scope="page" />												&#8364;${totalCycle}										</td>								</tr>								<tr  >									<td>										${company.testcasesDisplayName} 									</td>									<td align="right">											<c:set var="companyTestcases" value="0" scope="page" />																				<c:set var="companyTestcases" value="${companyTestcases + fn:length(company.testcases)}" scope="page"/>																													${companyTestcases}									</td>									<td align="right">										<c:set var="testcasePrice" value="0.38 " scope="page" />												&#8364;${testcasePrice} / Month<!-- &#8364; 0.12/hour -->									</td>									<td align="right">										<c:set var="totalTestcase" value="${companyTestcases * testcasePrice}" scope="page" />												&#8364;${totalTestcase}																			</td>								</tr>								<c:set var="testruncount" value="0" scope="page" />									<c:forEach var="testcase" items="${company.testcases}" varStatus="testcaseIndex" >																				<c:set var="testruncount" value="${testruncount + fn:length(testcase.testruns)}" scope="page"/>									</c:forEach>																<tr style="background-color:#edf7f9" >									<td>										${company.testrunsDisplayName} 									</td>									<td align="right">																																						${testruncount}									</td>									<td align="right">										<c:set var="testrunPrice" value="0.15 " scope="page" />												&#8364;${testrunPrice} / Month<!-- &#8364; 0.12/hour -->									</td>									<td align="right">										<c:set var="totalTestrun" value="${testruncount * testrunPrice}" scope="page" />												&#8364;${totalTestrun}									</td>								</tr>								<tr>									<td>										${company.defectsDisplayName} 									</td>									<td align="right">											<c:set var="companyDefects" value="0" scope="page" />																				<c:set var="companyDefects" value="${companyDefects + fn:length(company.defects)}" scope="page"/>																													${companyDefects}									</td>									<td align="right">										<c:set var="defectPrice" value="0.10" scope="page" />												&#8364;${defectPrice} / Month<!-- &#8364; 0.12/hour -->									</td>									<td align="right">										<c:set var="totalDefect" value="${companyDefects * defectPrice}" scope="page" />												&#8364;${totalDefect}																			</td>								</tr>								<tr style="background-color:#edf7f9">									<td>										${company.requirementsDisplayName} 									</td>									<td align="right">											<c:set var="companyRequirements" value="0" scope="page" />																				<c:set var="companyRequirements" value="${companyRequirements + fn:length(company.requirements)}" scope="page"/>																													${companyRequirements}									</td>									<td align="right">										<c:set var="requirementPrice" value="0.10" scope="page" />												&#8364;${requirementPrice} / Month<!-- &#8364; 0.12/hour -->									</td>									<td align="right">										<c:set var="totalRequirement" value="${companyRequirements * requirementPrice}" scope="page" />												&#8364;${totalRequirement}																			</td>								</tr>								<tr>									<td>										${company.environmentsDisplayName} 									</td>									<td align="right">											<c:set var="companyEnvironments" value="0" scope="page" />																				<c:set var="companyEnvironments" value="${companyEnvironments + fn:length(company.environments)}" scope="page"/>																													${companyEnvironments}									</td>									<td align="right">										<c:set var="environmentPrice" value="0.10" scope="page" />												&#8364;${environmentPrice} / Month<!-- &#8364; 0.12/hour -->									</td>									<td align="right">										<c:set var="totalEnvironment" value="${companyEnvironments * environmentPrice}" scope="page" />												&#8364;${totalEnvironment}																			</td>								</tr>								<tr style="background-color:#edf7f9">									<td>										Uploaded Files									</td>									<td align="right">											0									</td>									<td align="right">										<c:set var="environmentPrice" value="0.24" scope="page" />												&#8364;${environmentPrice} / GB<!-- &#8364; 0.12/hour -->									</td>									<td align="right">										&#8364;0.0																</td>								</tr>								<tr>									<td style="color: white;background-color: #6199DF;">										Total / Month									</td>									<td colspan="3" align="right">											<c:set var="TotalPriceDue" value="${totalEnvironment + totalRequirement + totalDefect + totalTestrun + totalTestcase + totalCycle + totalProject}" scope="page" />											&#8364;${TotalPriceDue} : <fmt:formatNumber value="${TotalPriceDue}" maxFractionDigits="0"/>																		</td>																	</tr>								</table>															</td>						</tr>									</table>										</td>			</tr>			</table>				</fieldset>					</div>			<div id="footer"></div>			</div>	<div id="preload">		<img src="images/title.png">	</div></body></html>	    	